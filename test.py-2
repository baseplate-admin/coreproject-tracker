import struct


def parse_udp_packet(msg):
    # connectionId: 64-bit (8 bytes)
    connection_id = msg[:8]

    # action: 32-bit unsigned integer (starting at byte 8)
    action = struct.unpack(">I", msg[8:12])[0]

    # transactionId: 32-bit unsigned integer (starting at byte 12)
    transaction_id = struct.unpack(">I", msg[12:16])[0]

    # Construct the result (similar to the JavaScript object)
    params = {
        "connectionId": connection_id,
        "action": action,
        "transactionId": transaction_id,
        "type": "udp",
    }

    return params


